@model ICollection<User>

<div class="add-user-dialog">
    @if (Model.Count > 0)
    { 
        <div class="input-control select">
            <select class="observer-select">
                @foreach (User user in Model)
                {         
                    <option value="@user.Id">@user.Login</option>
                }
            </select>
        </div>
        <span class="result label" style="display: none"></span>
    }
    else
    { 
        <h4>Не найдено ни одного заказчика</h4>
    }
</div>
